{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": [
    "airbnb"
  ],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "indent":[
        "error",
        2
    ],
    "camelcase": "off",
    "consistent-return": "off",
    "default-param-last": "off",
    "global-require": "off",
    "linebreak-style": "off",
    "no-console": "off",
    "no-fallthrough": "off",
    "no-param-reassign": "off",
    "no-return-assign": "off",
    "no-throw-literal": "off",
    "no-underscore-dangle": "off",
    "object-curly-newline": "off"
  }
}
